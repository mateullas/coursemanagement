    <connection>sakai29</connection>
    <sql>select distinct anyaca, codi_ass 
from (
SELECT DISTINCT
 A.ANYACA
,A.CODI_ASS||'-'||G.CPU_CODALF CODI_ASS
,pla.codi_pla
FROM TUDL_F_CV_ASSOFE A, TUDL_F_CV_ANY_ANYACA C, TUDL_F_CV_GRUPASSIG_CAMPUS G, tudl_F_cv_plaassofe pla
WHERE
    A.ANYACA = C.ANYACA
AND	C.IS_CURRENT='S'
AND G.CODI_ASS = a.codi_ass
AND g.anyaca = a.anyaca
AND G.PRINCIPAL = 'S'
AND G.MATRICULABLE = 'S'
AND G.RESPONSABLE = 'S'
and pla.anyaca = a.anyaca
and pla.codi_ass = a.codi_ass
AND (A.ANYACA, A.CODI_ASS||'-'||G.CPU_CODALF) NOT IN (SELECT DISTINCT ANYACA,CODI_ASS FROM UDL_CM_ESTATS_ASSIGNATURA)
/*Treiem les assignatures INEFC pq s'han de crear a m√† amb una altra plantilla*/
and a.codi_ass not like '800___'
/*Treiem els centres 9, 13, 14, 15, 16, 80, 888*/
and (pla.codi_pla,a.codi_ass) not in 
(
select distinct aso.codi_pla, aso.codi_ass
from 
  tudl_F_cv_titulacions tit, 
  talu_cpsplaimparticio@migra cen,   
  tudl_F_cv_plaassofe aso, 
  tudl_F_cv_assofe a 
where 
    tit.codi_pla = cen.pla_codalf 
and aso.anyaca = (select anyaca from tudl_F_cv_any_anyaca where is_current = 'S')
and aso.codi_pla = tit.codi_pla 
and a.anyaca = aso.anyaca 
and a.codi_ass = aso.codi_ass
and cen.cen_codnum in (9,13,14,15,16,80,888)
)
/*Treiem les assignatures de plans que no tenen centre assignat*/
and a.codi_ass not in 
(select distinct  aso.codi_ass
from 
  tudl_F_cv_titulacions tit, 
  tudl_F_cv_plaassofe aso, 
  tudl_F_cv_assofe a 
where 
    aso.anyaca = (select anyaca from tudl_F_cv_any_anyaca where is_current = 'S')
and aso.codi_pla = tit.codi_pla 
and a.anyaca = aso.anyaca 
and a.codi_ass = aso.codi_ass 
and tit.codi_pla not in (select distinct pla_codalf from talu_cpsplaimparticio@migra)
)
)</sql>
