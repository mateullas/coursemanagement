      <name>Esborra TOT</name>
      <description />
      <type>SQL</type>
      <sql>--COURSE_SET COURSE_OFFERING ASSOC
DELETE FROM UDL_CM_CSET_COFFERING_ASSOC;

--UPDATES MEMBER_CONTAINER
UPDATE UDL_CM_MEMBER_CONTAINER SET COURSE_OFFERING = NULL;
UPDATE UDL_CM_MEMBER_CONTAINER SET ACADEMIC_SESSION = NULL;
UPDATE UDL_CM_MEMBER_CONTAINER SET ENROLLMENT_SET = NULL;

--MEMBERSHIP
DELETE FROM UDL_CM_MEMBERSHIP;

--ENROLLMENT
DELETE FROM UDL_CM_ENROLLMENT;

--OFFICIAL INSTRUCTORS
DELETE FROM UDL_CM_OFFICIAL_INSTRUCTORS;

--ENROLLMENT SET
DELETE FROM UDL_CM_ENROLLMENT_SET;

--MEMBER_CONTAINER
DELETE FROM UDL_CM_MEMBER_CONTAINER ;

--ACADEMIC SESSION
DELETE FROM UDL_CM_ACADEMIC_SESSION ;

--SECTION CATEGORIES
DELETE FROM UDL_CM_SEC_CATEGORY;


--ESBORRAR SEQUENCES
DROP SEQUENCE UDL_CM_ACADEMIC_SESSION_S;
DROP SEQUENCE UDL_CM_ENROLLMENT_S;
DROP SEQUENCE UDL_CM_ENROLLMENT_SET_S;
DROP SEQUENCE UDL_CM_MEMBER_CONATINER_S;
DROP SEQUENCE UDL_CM_MEMBERSHIP_S;</sql>
