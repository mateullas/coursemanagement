;


--TOTS ELS ENROLLMENT_SET DE TEST
select enrollment_set_id from udl_F_CM_enrollment_set where
(enterprise_id like '%TIT1%' or enterprise_id like '%DOT1%')or
(enterprise_id like '1_' ) or
(enterprise_id like '1_-I' ) or
