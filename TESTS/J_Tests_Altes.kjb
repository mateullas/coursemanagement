(enterprise_id like '1__')
;
*/

--COURSE_SET COURSE_OFFERING ASSOC
DELETE FROM UDL_F_CM_CSET_COFFERING_ASSOC where course_set in
(select member_container_id from udl_F_CM_member_container where (enterprise_id like '%TIT1%' or enterprise_id like '%DOT1%') and class_discr like '%Set%');


--UPDATES MEMBER_CONTAINER
UPDATE UDL_F_CM_MEMBER_CONTAINER SET COURSE_OFFERING = NULL 
WHERE
(enterprise_id like '%TIT1%' or enterprise_id like '%DOT1%')or
(enterprise_id like '1_' ) or
(enterprise_id like '1_-I' ) or
(enterprise_id like 'TA-__' ) or
(enterprise_id IN ('101','111','121','132','112','131')) or
(category like '%ET-TIT1%') ;


UPDATE UDL_F_CM_MEMBER_CONTAINER SET ACADEMIC_SESSION = NULL
WHERE
(enterprise_id like '%TIT1%' or enterprise_id like '%DOT1%')or
(enterprise_id like '1_' ) or
(enterprise_id like '1_-I' ) or
(enterprise_id like 'TA-__' ) or
(enterprise_id IN ('101','111','121','132','112','131')) or
(category like '%ET-TIT1%');



UPDATE UDL_F_CM_MEMBER_CONTAINER SET ENROLLMENT_SET = NULL
WHERE
(enterprise_id like '%TIT1%' or enterprise_id like '%DOT1%')or
(enterprise_id like '1_' ) or
(enterprise_id like '1_-I' ) or
(enterprise_id like 'TA-__' ) or
(enterprise_id IN ('101','111','121','132','112','131'));

--MEMBERSHIP
DELETE FROM UDL_F_CM_MEMBERSHIP WHERE MEMBER_CONTAINER_ID IN (
select member_container_id from udl_F_CM_member_container where 
(enterprise_id like '%TIT1%' or enterprise_id like '%DOT1%')or
(enterprise_id like '1_' ) or
(enterprise_id like '1_-I' ) or
(enterprise_id like 'TA-__' ) or
(enterprise_id like '1__') or
(category like '%ET-TIT1%' ) 
);

--ENROLLMENT
DELETE FROM UDL_F_CM_ENROLLMENT where ENROLLMENT_SET IN (
select enrollment_set_id from udl_F_CM_enrollment_set where
(enterprise_id like '%TIT1%' or enterprise_id like '%DOT1%')or
(enterprise_id like '1_' ) or
(enterprise_id like '1_-I' ) or
(enterprise_id like 'TA-__' ) or
(enterprise_id IN ('101','111','121','132','112','131'))
);


--OFFICIAL INSTRUCTORS
DELETE FROM UDL_F_CM_OFFICIAL_INSTRUCTORS WHERE ENROLLMENT_SET_ID IN (
