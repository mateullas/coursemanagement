      <yloc>192</yloc>
    </entry>
    <entry>
      <name>Comprovar contenidors ET Cursos</name>
      <description>A ET pertany al grup corresponent a tercer</description>
      <type>EVAL_TABLE_CONTENT</type>
      <connection>sakai29</connection>
      <schemaname />
      <tablename />
      <success_condition>rows_count_equal</success_condition>
      <limit>0</limit>
      <is_custom_sql>Y</is_custom_sql>
      <is_usevars>N</is_usevars>
      <custom_sql>select enterprise_id from udl_cm_member_container where member_container_id in (
select member_Container_id from udl_Cm_membership where user_id = 'alumne18')
and enterprise_id like 'ET-TIT1-%'
minus
(select 
'ET-TIT1-3' ENTERPRISE_ID
from dual
)</custom_sql>
      <add_rows_result>N</add_rows_result>
      <clear_result_rows>Y</clear_result_rows>
      <parallel>N</parallel>
      <draw>Y</draw>
      <nr>0</nr>
      <xloc>624</xloc>
      <yloc>192</yloc>
    </entry>
    <entry>
      <name>Comprovar contenidors ET-13</name>
      <description>A ET pertany al grup corresponent a l'assignatura 13 i al grup 131</description>
      <type>EVAL_TABLE_CONTENT</type>
      <connection>sakai29</connection>
      <schemaname />
      <tablename />
      <success_condition>rows_count_equal</success_condition>
      <limit>0</limit>
      <is_custom_sql>Y</is_custom_sql>
      <is_usevars>N</is_usevars>
      <custom_sql>select category from udl_cm_member_container where member_container_id in (
select member_Container_id from udl_Cm_membership where user_id = 'alumne18')
and category like '%ET-TIT1-13%'
minus
(select 
'GRUP_ET:ET-TIT1-13:3' CATEGORY
from dual
UNION
select 
'GRUP_ET:ET-TIT1-13-131:3' CATEGORY
from dual
)</custom_sql>
